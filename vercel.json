{
  "framework": "gatsby",
  "buildCommand": "yarn build:vercel",
  "outputDirectory": "public",
  "installCommand": "yarn install",
  "functions": {
    "api/*.js": {
      "runtime": "nodejs16.x"
    }
  },
  "build": {
    "env": {
      "NODE_VERSION": "16",
      "NODE_OPTIONS": "--openssl-legacy-provider"
    }
  }
}
