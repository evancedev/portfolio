{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "^/(.*)",
      "dest": "/",
      "status": 200
    }
  ],
  "build": {
    "env": {
      "NODE_VERSION": "18",
      "NODE_OPTIONS": "--openssl-legacy-provider"
    }
  }
}
