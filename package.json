{
  "name": "v4",
  "description": "Evance Odhiambo's AI-focused Portfolio",
  "version": "1.0.0",
  "author": "Evance Odhiambo <evansodhams@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/EvansOdhams/portfolio"
  },
  "keywords": [
    "gatsby",
    "portfolio",
    "ai",
    "react"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "vercel": {
    "env": {
      "NODE_VERSION": "18",
      "NODE_OPTIONS": "--openssl-legacy-provider"
    }
  },
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "build:vercel": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "resolutions": {
    "graphql": "^14.7.0"
  },
  "dependencies": {
    "animejs": "^3.1.0",
    "assert": "^2.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.12.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "dotenv": "^17.2.3",
    "gatsby": "^5.11.0",
    "gatsby-plugin-google-gtag": "^5.13.0",
    "gatsby-plugin-image": "^3.11.0",
    "gatsby-plugin-manifest": "^5.11.0",
    "gatsby-plugin-netlify": "^5.1.0",
    "gatsby-plugin-offline": "^6.11.0",
    "gatsby-plugin-react-helmet": "^6.11.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sharp": "^5.11.0",
    "gatsby-plugin-sitemap": "^6.11.0",
    "gatsby-plugin-styled-components": "^6.11.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^8.11.0",
    "gatsby-remark-prismjs": "^7.11.0",
    "gatsby-source-filesystem": "^5.11.0",
    "gatsby-source-github-api": "^1.0.0",
    "gatsby-transformer-remark": "^6.11.0",
    "gatsby-transformer-sharp": "^5.11.0",
    "lodash": "^4.17.19",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prismjs": "^1.27.0",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-transition-group": "^4.3.0",
    "scrollreveal": "^4.0.5",
    "stream-browserify": "^3.0.0",
    "styled-components": "^5.3.0",
    "util": "^0.12.5"
